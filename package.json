{
  "name": "smashmate",
  "version": "4.0.0",
  "description": "A web application for Smash Ultimate nerds interested in seeing the raw data behind the Fighters.",
  "scripts": {
    "start": "npm run --prefix server start",
    "test": "npm run --prefix client test",
    "coverage": "npm run --prefix client coverage",
    "build": "npm-run-all build:*",
    "build:client": "npm run --prefix client build",
    "build:server": "npm run --prefix server tsc",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "npm run --prefix client dev",
    "dev:server": "npm run --prefix server dev:server",
    "tsc:watch": "npm run --prefix server tsc:watch",
    "postinstall": "npm-run-all install:*",
    "install:client": "npm --prefix client ci",
    "install:server": "npm --prefix server ci",
    "preview": "npm --prefix client preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreSean-Sutton/smashmate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DreSean-Sutton/smashmate/issues"
  },
  "homepage": "https://github.com/DreSean-Sutton/smashmate#readme",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "npm": "8.19.2",
    "node": "18.10.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
